<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=T, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles/style.css" />

    <title>show</title>
  </head>
  <body>
    <main>
      <a class="home" href="/">Home</a>
      <% books.forEach(book => {%>
      <section>
        <img src="<%= book.image %>" />
        <h3><%=book.title %></h3>
        <h4><%= book.authors %></h4>
        <p><%= book.description %></p>
      </section>
      <%})%>
    </main>
  </body>
</html>
